<% content_for(:title) { "My Dashboard" } %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="weather-geelong-west" data-view="Weather" data-title="Weather" style="background-color:#18A3FF"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="weather-melbourne" data-view="Weather" data-title="Weather" style="background-color:#18A3FF"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="internet" data-view="Usage" data-title="Internet" style="background-color:#8718FF"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="downloads" data-unordered="true" data-view="List" data-title="Downloads" style="background-color:#FF18A3"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="email" data-unordered="true" data-view="List" data-title="Email" style="background-color:#18FF6B"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="server-deathstar-cpu" data-view="RemoteImage" data-title="Deathstar CPU" style="background-color:#FF7E18"></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="server-deathstar-memory" data-view="RemoteImage" data-title="Deathstar Memory" style="background-color:#FF7E18"></div>
    </li>
  </ul>
</div>
